import random

number = random.randint(1, 100)

---

import random

for i in range(10):
  print(random.randint(1, 100))

alternatively,

import random

for i in range(10):
  number = random.randint(1, 100)
  print(number)

---

def roll_d20():
  import random
  dice = random.randint(1, 20)
  print("You rolled a", dice)

roll_d20()

---

import random

list_of_words = ["english", "stranger", "sublime", "water"]

word = random.choice(list_of_words) # Chooses a word at random from the list

print(word)
